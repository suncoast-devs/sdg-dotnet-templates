<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SDG.templates.Console</id>
    <version>1.2.2</version>
    <description>
      Creates a simple console app for use at Suncoast Developers Guild in Tampa Bay FL. Includes gitignore and tasks to interact with github.
    </description>
    <authors>Mark Dewey</authors>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
    <license type="expression">MIT</license>
  </metadata>
   <files>
    <file
      src="SampleApp/**/*"
      target="SampleApp" />
    <file
    src="SampleApp/.gitignore"/>
  </files>
</package>